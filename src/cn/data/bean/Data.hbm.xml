<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="cn.data.bean.Data" table="data">
		<id name="dataId" type="java.lang.Integer">
			<column name="dataId" />
			<generator  class="native"/>
		</id>
		<property name="data" type="java.lang.String">
			<column name="data" />
		</property>
		<property name="time" type="java.lang.String">
			<column name="time" />
		</property>
		<property name="equipmentId" type="java.lang.Integer">
			<column name="equipmentId" />
		</property>
		<property name="equipmentName" type="java.lang.String">
			<column name="equipmentName" />
		</property>
	</class>
</hibernate-mapping>